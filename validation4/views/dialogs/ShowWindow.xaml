<Window x:Class="validation4.views.dialogs.ShowWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:validation4.views.dialogs"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="ShowWindow" Height="500" Width="500">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <Frame NavigationUIVisibility="Hidden" Content="{Binding NavigatorOnDialog.CurrentVMDialog}" />
        </ScrollViewer>
       
    </Grid>
</Window>
